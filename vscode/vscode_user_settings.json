// Place your settings in this file to overwrite the default settings
{

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/*.js.map": {"when": "$(basename)"},
        "**/*.js": {"when": "$(basename).ts"}
        //"**/*.spec.ts": {"when": "$(basename).ts"}
    },
    "explorer.openEditors.visible": 0,
    "git.enabled": false,
     "git.autofetch": false,
     "editor.formatOnType": true
     
}